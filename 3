#include <iostream>
#include <cmath>
using namespace std;
int main()
{
	setlocale(LC_ALL, "rus");
	int a[] = { 1,2,3,4,5,6,7,8,10 };
	int n = 10, count = 0;
	double summ = 0;
	for (int i = 1;i < n;i++)
	{
		if (i % 2 == 1)
		{
			count++;
			summ = summ + a[i];
		}
	}
	if (count > 0) 
	{
		cout << summ / count;
	}
	else
	{
		cout << "NO";
	}
	system("pause");
	return 0;
}
